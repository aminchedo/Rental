{
  "name": "rental-management-vercel",
  "version": "1.0.0",
  "description": "Vercel Rental Management System with PostgreSQL",
  "main": "api/index.js",
  "scripts": {
    "dev": "vercel dev",
    "build": "cd client && npm run build",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "db:migrate": "psql $POSTGRES_URL -f postgres-migration.sql",
    "db:export": "node scripts/export-d1-data.js",
    "verify": "node scripts/verify-migration.js",
    "type-check": "tsc --noEmit",
    "lint": "cd client && npm run lint",
    "setup": "npm install && cd client && npm install && cd .."
  },
  "dependencies": {
    "@vercel/node": "^3.0.0",
    "@vercel/postgres": "^0.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vercel": "^33.0.0"
  },
  "keywords": [
    "vercel",
    "serverless",
    "rental-management",
    "postgresql",
    "persian",
    "rtl"
  ],
  "author": "Rental Management System",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}